#!/bin/bash
#This script is executed at login

export EMO='<o__o>'

case `date +"%x"` in

   12/24*) FES='Meri Crisma!!!';;

   12/25*) FES='Meri Crisma!!!';;

   01/01*) FES='Japi Niu Yir!!!';;

   12/31*) FES='Japi Ol Yir!!!';;

   11/29*) FES='Japi Berdei!!!';;

   08/13*) FES='Japi Aniversari!!!';;

   08/12*) FES='Japi Aniversari Tumorro!!!';;

   01/15*) FES='Japi Berdei Mailov!!!';;

   01/15*) FES='Japi Berdei Mailov!!!';;

   *) FES='Not Moch Tudei!!!';;

esac

export FES
